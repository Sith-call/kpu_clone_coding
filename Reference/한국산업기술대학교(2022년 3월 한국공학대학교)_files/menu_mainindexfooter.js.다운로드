//사물함 팝업조회
function commonPopView(url, name){
  open_window(url, name, "100", "100", "620", "700", "", "", "", "auto", "");	
}

//공동컨텐츠
var commonUrl = "";
function commonCMS(url, name, popWidth, popHeight){
  var commonCMSUrl = commonUrl+url;
  open_window(commonCMSUrl, name, "100", "100", popWidth, popHeight, "", "", "", "auto", "");	
}

function moveFamily(obj){
  if(obj == "관련사이트바로가기"){
	  //
  }else if(obj == "http://www.kpu-sanhak.ac.kr/include/loginExec2.jsp"){
    sitesso.document.form_sanhakgo.submit();
  }else{
    window.open(obj);
  }
}

