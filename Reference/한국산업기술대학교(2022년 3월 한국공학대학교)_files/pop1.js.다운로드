		document.write('  $("#layer1176").bind("dragstop",function(event, ui){ $(this).fadeTo(500,1); }); '); 
		document.write('  	function close1176(){layer1176.style.display="none";}	 	'); 
		document.write('  	function closeWin1176(){layer1176.style.display="none" ;	'); 
		document.write('  			setCookie( "popName1176", "done" , 1)}		'); 
		document.write('  	function close7Win1176(){layer1176.style.display="none" ;	'); 
		document.write('  			setCookie( "popName1176", "done" , 7)}		'); 
		document.write('	function setCookie(name, value, expiredays){'); 
		document.write('		var todayDate = new Date();'); 
		document.write('		todayDate.setDate(todayDate.getDate() + expiredays);'); 
		document.write('		todayDate.setYear(todayDate.getFullYear());'); 
		document.write('		todayDate.setMonth(todayDate.getMonth());'); 
		document.write('		todayDate.setDate(todayDate.getDate());'); 
		document.write('		todayDate.setHours(0);'); 
		document.write('		todayDate.setMinutes(0);'); 
		document.write('		todayDate.setSeconds(0);'); 
		document.write('		document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";";'); 
		document.write('	}'); 
		document.write('  </script>'); 
		document.write('		<div class="pop_top" style="background:#1C6DC7;">'); 
